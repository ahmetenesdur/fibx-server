{
	"name": "fibx-server",
	"version": "0.1.5",
	"description": "Server implementation for fibx CLI",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/ahmetenesdur/fibx-server.git"
	},
	"bugs": {
		"url": "https://github.com/ahmetenesdur/fibx-server/issues"
	},
	"homepage": "https://github.com/ahmetenesdur/fibx-server#readme",
	"type": "module",
	"scripts": {
		"dev": "node --env-file=.env --import tsx/esm --watch src/index.ts",
		"build": "tsc",
		"start": "node dist/src/index.js",
		"typecheck": "tsc --noEmit",
		"lint": "eslint src/",
		"lint:fix": "eslint src/ --fix",
		"format": "prettier --write .",
		"format:check": "prettier --check ."
	},
	"dependencies": {
		"@hono/node-server": "^1.19.9",
		"@hono/zod-validator": "^0.7.6",
		"@privy-io/node": "^0.8.0",
		"hono": "^4.12.2",
		"jose": "^6.1.3",
		"zod": "^3.25.76"
	},
	"devDependencies": {
		"@eslint/js": "^9.39.3",
		"@types/node": "^22.19.11",
		"eslint": "^9.39.3",
		"eslint-config-prettier": "^10.1.8",
		"globals": "^17.3.0",
		"prettier": "^3.8.1",
		"tsx": "^4.21.0",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.56.1"
	}
}
